# SnapChef Environment Variables
# Copy this file to .env and fill in your actual API keys

# Google Cloud Vision API Key
EXPO_PUBLIC_GOOGLE_CLOUD_API_KEY=your_google_cloud_vision_api_key_here

# Spoonacular Recipe API Key (optional - for enhanced recipe data)
EXPO_PUBLIC_SPOONACULAR_API_KEY=your_spoonacular_api_key_here

# OpenAI API Key
# Get from: https://platform.openai.com/api-keys
OPENAI_API_KEY=your_openai_api_key_here

# Optional: App Configuration
APP_ENV=development
API_TIMEOUT=30000



